FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;
interpolationScheme cellPoint;

fields
(
    Temperature
);

sets
(
    sample
    {
        type nearestTriSurfaceMeshPointSet;
        axis xyz;
        surface "SOLIDF.obj";
    }
);

